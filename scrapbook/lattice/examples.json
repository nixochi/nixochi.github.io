    },
    {
      "name": "Fractal Noise",
      "equation": "sin(x*4 + t)*0.3 + sin(x*8 + y*2 + t*1.5)*0.2 + sin(x*16 + y*4 + z*2 + t*2)*0.15 + sin(z*12 + t*0.7)*0.15 + 0.5",
      "description": "Multi-scale fractal-like noise pattern"
    },
    {
